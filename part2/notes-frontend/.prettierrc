{
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxSingleQuote": true,
  "quoteProps": "as-needed",
  "useTabs": false,
  "tabWidth": 2,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "semi": true,
        "singleQuote": false,
        "trailingComma": "none",
        "bracketSpacing": false,
        "arrowParens": "always"
      }
    }
  ]
}
